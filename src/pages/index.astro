					</div>
				</div>

				<script is:inline>
					let tickers = document.querySelectorAll('.ticker span')

					tickers[0].classList.add('active')

					let i = 0;

					function passTicker() {
						
						if(i == (tickers.length)) {
							i = 0;
							document.querySelector('.active').classList.remove('active')
						}
						
						if(i > 0) {
							tickers[i - 1].classList.remove('active')
						}
						
						tickers[i].classList.add('active')
						i++
					}

					setInterval(() => {
						passTicker()
					}, 5000);

					passTicker()
				</script>

